#AUTOMATIC START
echo "Desc: UDPGW Enabler[auto-starter]"
echo "Author: Tesla SSH"
echo "Date: 20/09/23"
echo "============="
echo "UDPGW -ON"

screen -dmS udpgw /bin/udpgw --listen-addr 127.0.0.1:7300 --max-clients 1000 --max-connections-for-client 100